create database bookstoredb;

use bookstoredb;

create table book (
	id INT auto_increment,
    title varchar(20),
    publisher varchar(10),
    author varchar(10),
    checkout bool default false,
    primary key(id)
);

insert into book(title, publisher, author, checkout)
	values("객체지향 시스템개론", "헤헤출판사", "짱승", FALSE);
	
use bookstoredb;
create table admin (
	id varchar(20),
    pw varchar(20),
    primary key(id)
);

insert into admin values("admin", "admin");